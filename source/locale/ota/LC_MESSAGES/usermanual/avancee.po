#: ../usermanual/avancee.rst:111 dbdee85f9a0f45a595c8a21171b4872d
msgid "the columns to export,"
msgstr ""

# d6aba5be13df4a5795c067c3526efd39
#: ../usermanual/avancee.rst:112 d6aba5be13df4a5795c067c3526efd39
